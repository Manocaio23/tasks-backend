pipeline{
    agent any
    stages{
        stage ('Build Backend'){
            steps{
                bat 'mvn clean package -DskipTests=true'
            }
        }
        stage ('Unity Tests'){
            steps{
                bat 'mvn test'
            }
        }

        stage ('Sonar Analysis'){
            enviroment{
                scannerHome = tool 'SONAR_SCANNER'
            }
            steps{
                withSonarQubeEnv('SONAR_LOCAL'){
                    bat "${scannerHome}/bin/sonar-scanner -e -Dsonar.projectKey=DeployBack -Dsonar.host.url=http://localhost:9000 -Dsonarnar.login=4355f47c2562e1d8c610366e976ab62785d8d9b8 -Dsonarnar.java.binaries=target"
                }
        
                
            }
        }
    }
}

